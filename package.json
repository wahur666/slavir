{
  "name": "slavir",
  "version": "0.0.1",
  "description": "Open-source Real Time Tactics Game",
  "main": "index.js",
  "scripts": {
    "build:server": "tsc --build tsconfig.server.json",
    "build:client": "webpack",
    "watch:client": "webpack --watch",
    "watch:server": "tsc --build tsconfig.server.json -w --preserveWatchOutput",
    "nodemon": "nodemon ./out/index.js",
    "dev": "npm run build:server && npm-run-all --parallel  watch:server watch:client nodemon "
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wahur666/slavir.git"
  },
  "keywords": [
    "phaser3",
    "game"
  ],
  "author": "Szecsodi Imre",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/wahur666/slavir/issues"
  },
  "homepage": "https://github.com/wahur666/slavir#readme",
  "dependencies": {
    "express": "^4.17.1",
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.9.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "4.5.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpackbar": "^5.0.2"
  }
}
